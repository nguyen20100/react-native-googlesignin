{
  "expo": {
    "name": "GoogleSignin",
    "slug": "google-login",
    "privacy": "public",
    "sdkVersion": "36.0.0",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "example.expo.googlesignin",
      "config": {
        "googleSignIn": {
          "reservedClientId": "com.googleusercontent.apps.829364964290-5dohs4ftal80csh8g4v9932jogs9jor3"
        }
      }
    },
    "android":{
      "package": "example.expo.googlesignin",
      "googleServicesFile": "./google-services.json"
    }
  }
}
